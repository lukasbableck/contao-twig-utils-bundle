{
    "name": "lukasbableck/contao-twig-utils-bundle",
    "type": "contao-bundle",
	"description": "This bundle provides some useful additional twig functions and filters for Contao Open Source CMS.",
	"keywords": [
		"backend",
		"twig",
		"filter",
		"function"
	],
    "license": "LGPL-3.0-or-later",
	"authors": [
		{
			"name": "Lukas Bableck",
			"email": "lukas@bableck.dev",
			"homepage": "https://bableck.dev"
		}
	],
    "require": {
        "php": "^8.1",
        "contao/core-bundle": "^5.3"
    },
	"conflict": {
		"contao/manager-plugin": "<2.0 || >=3.0"
	},
	"require-dev": {
		"contao/manager-plugin": "^2.0",
		"friendsofphp/php-cs-fixer": "^3.57"
	},
    "autoload": {
        "psr-4": {
            "Lukasbableck\\ContaoTwigUtilsBundle\\": "src/"
        }
    },
	"extra": {
		"contao-manager-plugin": "Lukasbableck\\ContaoTwigUtilsBundle\\ContaoManager\\Plugin"
	}
}
